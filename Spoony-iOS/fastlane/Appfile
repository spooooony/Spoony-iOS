apple_id(ENV['APPLE_ID']) # Your Apple Developer Portal username

team_id(ENV['TEAM_ID']) # Developer Portal Team ID
itc_team_id(ENV['FASTLANE_ITC_TEAM_ID']) # App Store Connect Team

